{
	"flex": {
		"style": {
			"display": "flex"
		}
	},
	"flex-1": {
		"style": {
			"flexGrow": 1,
			"flexShrink": 1,
			"flexBasis": "0%"
		}
	},
	"items-center": {
		"style": {
			"alignItems": "center"
		}
	},
	"justify-center": {
		"style": {
			"justifyContent": "center"
		}
	},
	"rounded-full": {
		"style": {
			"borderTopLeftRadius": 9999,
			"borderTopRightRadius": 9999,
			"borderBottomRightRadius": 9999,
			"borderBottomLeftRadius": 9999
		}
	},
	"bg-black": {
		"style": {
			"--tw-bg-opacity": 1,
			"backgroundColor": "rgb(0 0 0 / var(--tw-bg-opacity))"
		}
	},
	"bg-white": {
		"style": {
			"--tw-bg-opacity": 1,
			"backgroundColor": "rgb(255 255 255 / var(--tw-bg-opacity))"
		}
	},
	"px-3": {
		"style": {
			"paddingLeft": 12,
			"paddingRight": 12
		}
	},
	"py-1": {
		"style": {
			"paddingTop": 4,
			"paddingBottom": 4
		}
	},
	"font-semibold": {
		"style": {
			"fontWeight": "600"
		}
	},
	"text-white": {
		"style": {
			"--tw-text-opacity": 1,
			"color": "rgb(255 255 255 / var(--tw-text-opacity))"
		}
	},
	"dark:bg-white": {
		"style": {
			"--tw-bg-opacity": 1,
			"backgroundColor": "rgb(255 255 255 / var(--tw-bg-opacity))"
		},
		"media": "(prefers-color-scheme: dark)"
	},
	"dark:bg-red-500": {
		"style": {
			"--tw-bg-opacity": 1,
			"backgroundColor": "rgb(239 68 68 / var(--tw-bg-opacity))"
		},
		"media": "(prefers-color-scheme: dark)"
	},
	"dark:text-blue-800": {
		"style": {
			"--tw-text-opacity": 1,
			"color": "rgb(30 64 175 / var(--tw-text-opacity))"
		},
		"media": "(prefers-color-scheme: dark)"
	}
}